    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/popper")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/cookie")
    @Scripts.Render("~/bundles/jqueryOthers")

    @Scripts.Render("~/bundles/sweetalert")
    @Scripts.Render("~/bundles/carousel")
    @Scripts.Render("~/bundles/js")
    <script>

        $(document).ready(function () {
            updateCardProducts();
        });

        $('#Card').hover(function () {
            var $this = $(this),
                timer = $this.data("timer") || 0;
            clearTimeout(timer);
            timer = setTimeout(GetCardProducts, 1000);
            $this.data("timer", timer);
        }, function () {
            var $this = $(this),
                timer = $this.data("timer") || 0;
            clearTimeout(timer);
        });

        $('.animate-top').click(function () {
            //var id = $(this).attr('data-Id');
            ProductsAddToCard($(this).attr('data-Id'));
        });
        function GetCardProducts() {
            $.ajax({
                url: '@Url.Action("CardProducts","Shop",new { area=""})',
                type: 'GET'
            }).done(function (response) {
                $(".cart-dropdown").html(response);
            }).fail(function (XHLHttpRequest, textStatus, errorThrown) {
                alert("FAIL");
            });
        };
        function ProductsAddToCard(Id) {
            var productIds;
            var exestingProductIds = $.cookie('CardProducts');
            if (exestingProductIds != null && exestingProductIds != undefined && exestingProductIds != " ") {
                productIds = exestingProductIds.split('-');
            } else {
                productIds = [];
            }
            var productId = Id;
            //var productId = $(this).attr('data-Id');
            productIds.push(productId);
            $.cookie('CardProducts', productIds.join('-'), { path: '/' });
            updateCardProducts()
            swal({
                title: "Done!",
                text: "Products Added to Card.",
                icon: "success",
                buttons: {
                    confirm: {
                        text: "OK",
                        value: true,
                        visible: true,
                        className: "btn btn-success",
                        closeModal: true
                    }
                }
            })

        }
        function updateCardProducts() {
            var exestingProductIds = $.cookie('CardProducts');
            if (exestingProductIds != null && exestingProductIds != undefined && exestingProductIds != " ") {
                productIds = exestingProductIds.split('-');
            } else {
                productIds = [];
            }

            $('#CardProductCount').html(productIds.length);
        }

    </script>
    <script>
        function showModal(Id) {
            $.ajax({
                url: '@Url.Action("Modal","Home",new { area=""})' + '?Id=' + Id

            }).done(function (response) {
                $("#exampleModal .modal-content").html(response);
            }).fail(function (XHLHttpRequest, textStatus, errorThrown) {
                alert("FAIL");
            });
        }
    </script>


